{
  "name": "www",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "^20.11",
    "npm": "^10.2.3"
  },
  "scripts": {
    "predev": "npm run build",
    "dev": "nodemon server/index.mjs --ignore ./server/routes/wordpress/private/data.json",
    "vdev": "vite",
    "build": "vite build",
    "reimg": "docker build -t kpa-www:latest .",
    "docker-start": "docker run --platform linux/amd64 -p 2999:2999 kpa-www:latest",
    "start": "node server/index.mjs"
  },
  "dependencies": {
    "axios": "1.7.2",
    "body-parser": "1.20.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.19.2",
    "express-rate-limit": "7.3.1",
    "helmet": "7.1.0",
    "lodash": "4.17.21",
    "luxon": "3.4.4",
    "src": "file:src",
    "string-similarity": "4.0.4",
    "vue": "3.4.29",
    "vue-router": "4.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "nodemon": "3.1.4",
    "vite": "^5.3.1"
  }
}
